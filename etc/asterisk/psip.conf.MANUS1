; =================================================================

; --- Transportes ---
[transport-udp]
type=transport
protocol=udp
bind=0.0.0.0:5060
external_media_address = 168.138.158.51
external_signaling_address = 168.138.158.51

; --- Templates de Configuração ---
[template-ramal](!)
type=endpoint
context=interno
disallow=all
allow=ulaw
allow=alaw
allow=gsm
rtp_symmetric=yes
force_rport=yes
rewrite_contact=yes
direct_media=no
aors=ramal-aor
auth=ramal-auth

[ramal-aor](!)
type=aor
max_contacts=1

[ramal-auth](!)
type=auth
auth_type=userpass
password=torra123

