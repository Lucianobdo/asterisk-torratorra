; =================================================================
; CONFIGURAÇÃO DO TRONCO SIP - E2NET (VERSÃO REVISADA)
; =================================================================

[e2net-trunk]
type=endpoint
transport=transport-udp
context=publico
disallow=all
allow=ulaw
allow=alaw
outbound_auth=e2net-auth
aors=e2net-aor
from_user=6137740802
from_domain=sip18.e2net.com.br
language=pt_BR
identify_by=auth_username

[e2net-auth]
type=auth
auth_type=userpass
password=P0c*6137740802
username=6137740802
realm=sip18.e2net.com.br

[e2net-aor]
type=aor
contact=sip:sip18.e2net.com.br:5060

[e2net-register]
type=registration
transport=transport-udp
outbound_auth=e2net-auth
server_uri=sip:sip18.e2net.com.br
client_uri=sip:6137740802@sip18.e2net.com.br
retry_interval=60

